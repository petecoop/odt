{
    "name": "petecoop/odt",
    "description": "Render .odt files with Blade, convert to pdf with Libre/OpenOffice",
    "license": "MIT",
    "authors": [
        {
            "name": "Pete Cooper",
            "email": "pete@petecoop.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Petecoop\\ODT\\": "src"
        }
    },
    "require": {
        "nelexa/zip": "^4.0",
        "symfony/css-selector": "^5.2|^6.0",
        "symfony/dom-crawler": "^5.2|^6.0",
        "spatie/temporary-directory": "^1.3|^2.1",
        "symfony/process": "^5.2|^6.0",
        "illuminate/view": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Petecoop\\ODT\\ServiceProvider"
            ],
            "aliases": {
                "ODT": "Petecoop\\ODT\\Facades\\ODT"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
